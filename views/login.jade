          | Sign In With
        a.btn-face.m-b-20(href='#')
          i.fa.fa-facebook-official
          | 						Facebook
        a.btn-google.m-b-20(href='#')
          img(src='images/icons/icon-google.png', alt='GOOGLE')
          | 						Google
        .p-t-31.p-b-9
          span.txt1
            | Username
        .wrap-input100.validate-input(data-validate='Username is required')
          input.input100(type='text', name='username')
          span.focus-input100
